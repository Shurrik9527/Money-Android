    </item>
    <item android:state_pressed="true">
        <shape>
            <!--<stroke android:width="0.3dip" android:color="@color/grey" />-->
            <solid android:color="@color/grey_10" />
            <corners android:bottomLeftRadius="3dip" android:topLeftRadius="3dip" />
        </shape>
    </item>
    <item>
        <shape>
            <!--<stroke android:width="0.3dip" android:color="@color/grey" />-->
            <solid android:color="@color/grey_10" />
            <corners android:bottomLeftRadius="3dip" android:topLeftRadius="3dip" />
        </shape>
    </item>
